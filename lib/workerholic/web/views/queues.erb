<table class='table'>
  <thead>
    <tr>
      <th>Queue Name</th>
      <th>Job Load</th>
    </tr>
  </thead>

  <% @queues.each_with_index do |queue, i| %>
    <tr>
      <td id='queue_name_<%= queue[0].split(':').last %>'><%= queue[0] %></td>
      <td id='queue_count_<%= queue[0].split(':').last %>'><%= queue[1] %></td>
    </tr>
  <% end %>

  <tr class='last_row'>
    <th>Total:</td>
    <td id="queue_total"><%= @total %></td>
  </tr>
</table>
